{
  "expo": {
    "name": "ADN-Gym",
    "slug": "ADN-Gym",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logo-gym.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/logo-gym.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.shadowr2909.mobileStart"
    },
    "web": {
      "favicon": "./assets/logo-gym.png"
    },
    "extra": {
      "eas": {
        "projectId": "2824da16-f478-470d-9dcb-589d8ad17c1e"
      }
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "La app accede a tu galería para cambiar tu foto de perfil.",
          "cameraPermission": "La app necesita acceso a la cámara para tomar fotos."
        }
      ],
      "expo-font"
    ]
  }
}
